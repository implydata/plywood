{
  "name": "plywood",
  "version": "0.22.0-6",
  "description": "A query planner and executor",
  "keywords": [
    "split",
    "apply",
    "combine",
    "druid",
    "query planner"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/plywood.git"
  },
  "main": "build/plywood.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/",
    "package/",
    "typings/"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf"
  },
  "scripts": {
    "prepare": "tdi -q",
    "compile": "./compile",
    "pretest": "./compile",
    "test": "./travis-test",
    "full-test": "./run-tests"
  },
  "tonicExampleFilename": "docs/examples/tonic.js",
  "dependencies": {
    "chronoshift": "^0.9.4",
    "druid-query-toolkit": "^0.7.7",
    "druid.d.ts": "^0.12.1",
    "has-own-prop": "^1.0.1",
    "immutable-class": "^0.9.4",
    "moment-timezone": "^0.5.26",
    "plywood-base-api": "^0.2.8",
    "readable-stream": "^3.0.3",
    "stream-to-array": "^2.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "awesome-code-style": "^1.4.3",
    "chai": "4.2.0",
    "f-tool": "0.1.3",
    "immutable-class-tester": "0.7.2",
    "imonit": "0.2.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "pegjs": "0.10.0",
    "plywood-druid-requester": "2.6.2",
    "plywood-mysql-requester": "2.3.0",
    "plywood-postgres-requester": "1.4.0",
    "tdi": "0.6.5",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  }
}
